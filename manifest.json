{
  "manifest_version": 2,
  "name": "let-me-scroll",
  "description": "Stops certain websites from disabling scrolling",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["let-me-scroll.css"]
    }
  ],
  "icons": {
    "48": "icons/cookie.png",
    "96": "icons/cookie@2x.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "let-me-scroll@faissaloo",
      "strict_min_version": "58.0"
    }
  }
}
